datasource db {
    provider = "sqlite"
    url      = "file:../data/db.sqlite"
}

generator client {
    provider = "cargo prisma"
    output   = "../src/prisma.rs"
}

model Message {
    id      String @id @default(cuid())
    content String
}

model User {
    id        String   @id @default(cuid())
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    name String
}
